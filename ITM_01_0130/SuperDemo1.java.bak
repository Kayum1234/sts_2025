class Parent
{
	Parent(){
		System.out.println("Parent class 0 param constructor");
	}
	
	Parent(int  a){
		System.out.println("Parent class int param constructor");
	}
}
class Child extends Parent
{
	Child(){
		super(10);
		super();
	}
}
class  SuperDemo1
{
	public static void main(String[] args) 
	{
		Child  c1=new Child();
	}
}
