 constructor chaining
 constructor overloading
 ==============================================
*/
class Test
{
	 int  a;
	 Test(int  a){
		 System.out.println(this.hashCode() +" address in constructor");
		this.a=a;
	 }
	 
	 void  getA(){
		System.out.println(this.hashCode() +" The value of a="+a);
	 }
}
class  ThisDemo1
{
	public static void main(String[] args) 
	{
		Test t1=new Test(10);
		Test t2=new Test(15);
		Test t3=new Test(20);
		System.out.println("t1 object ka address="+t1.hashCode());
		System.out.println("t2 object ka address="+t2.hashCode());
		System.out.println("t3 object ka address="+t3.hashCode());
		t3.getA();
		t2.getA();
		t1.getA();
	}
}
