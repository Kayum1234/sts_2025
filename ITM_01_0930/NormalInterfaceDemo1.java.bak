//Normal -arrange marriage
@FunctionalInterface
interface Flipkart
{
	void deliver(String productName);
}

class DTDC implements Flipkart
{
	public void deliver(String productName){
		System.out.println(productName+ " delivered by DTDC ");
	}
}
 
