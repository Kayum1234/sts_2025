class  NormalInterfaceDemo1
{
	public static void main(String[] args) 
	{
		List<Integer> marks = Arrays.asList(35, 50, 42, 67, 28, 90, 55);

        System.out.println("Java 8 - Passing Students with Grace Marks:");

        marks.stream().filter(m -> m > 40)         // Step 1: Filter
              .map(m -> m + 5)             // Step 2: Add grace marks
             .sorted()                    // Step 3: Sort
             .forEach(System.out::println); // Step 4: Print
    
	}
}
