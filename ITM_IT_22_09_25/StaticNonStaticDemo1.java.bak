class  Car
{
	int carId; //object se
	static String brandName;//ClassName+object se
	
	void displayCarData(){
		System.out.println("car id is="+carId);
		System.out.println("brand name is="+brandName);
	}
}

class StaticNonStaticDemo1
{
	public static void main(String[] args){
		Car  c1=new Car();
		Car  c2=new Car();
		
		c1.carId=1111;
		c1.brandName="Hyundi";
		c2.brandName="Maruti suzuki";
		
		c1.displayCarData();
		c2.displayCarData();
	}
}
